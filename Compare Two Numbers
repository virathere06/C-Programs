#include<iostream>
using namespace std;

int main(){

int num1, num2;

cout << "Enter Number 1" << endl;
cin >> num1;

cout << "Enter Number 2" << endl;
cin >> num2;

if(num1 == num2){
  
  cout << "Both Numbers are Equal" << endl;
}
else{
  cout << "Both Numbers are Not Equal" << endl;
}

return 0;
}
